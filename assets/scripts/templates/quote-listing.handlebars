{{#each quotes as |quote|}}
<div class="quote" data-id="{{quote.id}}">
  <h1> <small>Quote: {{quote.author}}</small></h1>
  <div>
    {{#if quote.user}}
      {{quote.user.email}}
    {{else}}
      Unknown user.
    {{/if}}
  </div>
  <div>
    {{#if quote.quoteText}}
      {{quote.quoteText}}
    {{else}}
      Unknown quoteText.
    {{/if}}
  </div>
</div>

{{/each}}
